{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "noEmit": true,
    "baseUrl": "src",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
      "theme": ["ui/theme"],
      "screens": ["screens"]
    },
  }
}
